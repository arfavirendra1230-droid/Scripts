-- Scripts:

local function BSGGM_fake_script() -- DBackdoor.LocalScript 
	local script = Instance.new('LocalScript', DBackdoor)

	local main = script.Parent.Main
	local btn = main.Parent.Frame.TextButton
	local plr = game.Players.LocalPlayer
	
	btn.MouseButton1Click:Connect(function()
		if workspace:FindFirstChild("DEV") then
			main.Visible = true
			btn.Parent.Visible = false
		else
			plr:Kick("No loadstring or Not supported game")
		end
	end)
	
	
	
end
coroutine.wrap(BSGGM_fake_script)()
local function LDRUUJ_fake_script() -- Main.Dragify 
	local script = Instance.new('LocalScript', Main)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.50), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(LDRUUJ_fake_script)()
local function YBHY_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local btn = script.Parent
	local home = btn.Parent.Parent.Home
	local tos = btn.Parent.Parent.Tos
	local executor = btn.Parent.Parent.Executor
	
	btn.MouseButton1Click:Connect(function()
		home.Visible = true
		tos.Visible = false
		executor.Visible = false
	end)
end
coroutine.wrap(YBHY_fake_script)()
